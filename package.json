{
   "name": "server",
   "version": "0.0.1",
   "description": "Awesome project developed with TypeORM.",
   "private": true,
   "devDependencies": {
      "@types/node": "^8.0.29",
      "@typescript-eslint/eslint-plugin": "^3.8.0",
      "@typescript-eslint/parser": "^3.8.0",
      "concurrently": "^5.3.0",
      "copy-and-watch": "^0.1.5",
      "eslint": "^7.6.0",
      "eslint-config-prettier": "^6.11.0",
      "eslint-plugin-prettier": "^3.1.4",
      "nodemon": "^2.0.4",
      "prettier": "^2.0.5",
      "ts-node": "3.3.0",
      "typescript": "3.3.3333",
      "typescript-eslint": "^0.0.1-alpha.0"
   },
   "dependencies": {
      "glob": "^7.1.6",
      "graphql-yoga": "^1.18.3",
      "merge-graphql-schemas": "1.7.7",
      "pg": "^7.3.0",
      "reflect-metadata": "^0.1.10",
      "typeorm": "0.2.25"
   },
   "scripts": {
      "prestart": "tsc",
      "start": "concurrently \"nodemon node build/index.js\" \"tsc --watch\" \"yarn cp-gql --watch\" ",
      "start:production": "node build/index.js",
      "build": "tsc && yarn cp-gql",
      "cp-gql": "copy-and-watch src/**/*.{gql,graphql} build"
   }
}
